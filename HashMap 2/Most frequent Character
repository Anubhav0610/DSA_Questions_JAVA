class Solution {
    public char getMaxOccuringChar(String s) {
        // code here
        int[] freq = new int[26]; 
        for (char c : s.toCharArray()) {
            freq[c - 'a']++;
        }
        int max = 0;
        char ans = 'a';
        
        for (int i = 0; i < 26; i++) {
            if (freq[i] > max) {
                max = freq[i];
                ans = (char)(i + 'a');
            }
        }
        
        return ans;
    
    }
}
