/**
 * Definition for a binary tree node.
 * public class TreeNode {
 *     int val;
 *     TreeNode left;
 *     TreeNode right;
 *     TreeNode() {}
 *     TreeNode(int val) { this.val = val; }
 *     TreeNode(int val, TreeNode left, TreeNode right) {
 *         this.val = val;
 *         this.left = left;
 *         this.right = right;
 *     }
 * }
 */
class Solution {
    List<Integer> ab=new ArrayList<>();
    public List<Integer> rightSideView(TreeNode root) {
        Listt(root,0);
        return ab;
    }
    void Listt(TreeNode root,int level)
    {
        if(root==null)
        return;
        if(level==ab.size())
        ab.add(root.val);
        Listt(root.right,level+1);
        Listt(root.left,level+1);
    }
}
