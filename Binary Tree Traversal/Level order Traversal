/**
 * Definition for a binary tree node.
 * public class TreeNode {
 *     int val;
 *     TreeNode left;
 *     TreeNode right;
 *     TreeNode() {}
 *     TreeNode(int val) { this.val = val; }
 *     TreeNode(int val, TreeNode left, TreeNode right) {
 *         this.val = val;
 *         this.left = left;
 *         this.right = right;
 *     }
 * }
 */
class Solution {
    public List<List<Integer>> levelOrder(TreeNode root) {
        List<List<Integer>> ab=new ArrayList<>();
        if(root==null)
        return ab;
        Queue<TreeNode> a=new LinkedList<>();
        a.add(root);
        while(!a.isEmpty())
        {
            int size=a.size();
            List<Integer> curr=new ArrayList<>();
            while(size>0)
            {
                TreeNode cur=a.poll();
                curr.add(cur.val);
                if(cur.left!=null)
                a.add(cur.left);
                if(cur.right!=null)
                a.add(cur.right);
                size--;
            }
            ab.add(curr);
        }
        return ab;
    }
}
